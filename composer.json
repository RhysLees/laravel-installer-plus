{
    "name": "rhyslees/laravel-installer-plus",
    "description": "Laravel Installer Plus",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://rhyslees.co.uk/laravel-installer-plus",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/rhyslees/laravel-installer-plus/issues",
        "source": "https://github.com/rhyslees/laravel-installer-plus"
    },
    "authors": [
        {
            "name": "Rhys Lees",
            "email": "43909932+RhysLees@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel-zero/framework": "^9.0",
        "laravel-zero/phar-updater": "^1.2",
        "nunomaduro/laravel-console-menu": "^3.2",
        "nunomaduro/termwind": "^1.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["laravel-installer-plus"]
}
